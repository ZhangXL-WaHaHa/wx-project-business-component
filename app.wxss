/**app.wxss**/
page {
	height: 100%;
}

.container {
	height: 100%;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: center;
	box-sizing: border-box;
}

.btn {
	margin-bottom: 20rpx;
}

/* 隐藏scroll-view滚动条 ios上测试存在部分机型没有隐藏 */
::-webkit-scrollbar {
	width: 0;
	height: 0;
	color: transparent;
	display: none;
}
